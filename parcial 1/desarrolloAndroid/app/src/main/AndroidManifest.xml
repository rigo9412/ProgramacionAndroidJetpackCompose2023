<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.DesarrolloAndroid"
        tools:targetApi="31">
        <activity
            android:name=".Pig"
            android:exported="false"
            android:label="@string/title_activity_pig"
            android:theme="@style/Theme.DesarrolloAndroid" />
        <activity
            android:name=".Creeper"
            android:exported="false"
            android:label="@string/title_activity_creeper"
            android:theme="@style/Theme.DesarrolloAndroid" />
        <activity
            android:name=".crearEsqueleto"
            android:exported="false"
            android:label="@string/title_activity_crear_esqueleto"
            android:theme="@style/Theme.DesarrolloAndroid" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.DesarrolloAndroid">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>